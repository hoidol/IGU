<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="65.7dp"
    android:elevation="0dp"
    android:id="@+id/route_Container">
    <com.guanaj.easyswipemenulibrary.EasySwipeMenuLayout
        android:id="@+id/es"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:contentView="@+id/content"
        app:rightMenuView="@+id/right"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:id="@+id/content"
            >
            <com.iguideu.custom_view.RoundedImageView
                android:layout_width="38.5dp"
                android:layout_height="39dp"
                android:id="@+id/profile_ImageView"
                android:layout_marginLeft="29.3dp"
                app:srcCompat="@mipmap/profile_image_test"
                android:scaleType="fitXY"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="39dp"
                android:gravity="center_vertical"
                android:layout_toRightOf="@+id/profile_ImageView"
                android:lines="1"
                android:id="@+id/route_title_TextView"
                android:ellipsize="end"
                android:layout_toLeftOf="@+id/rating_Container"
                android:layout_marginLeft="16.8dp"
                android:text="길거리 음식투어ddddddddddddddddddddddddddd"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="39dp"
                android:orientation="horizontal"
                android:layout_alignParentRight="true"
                android:id="@id/rating_Container"
                android:gravity="center_vertical"
                android:layout_marginRight="19.3dp">

                <ImageView
                    android:layout_width="12.3dp"
                    android:layout_height="12dp"
                    android:layout_marginRight="9.7dp"
                    android:background="@mipmap/star_solid"
                    android:id="@+id/rating_star_0"
                    />
                <ImageView
                    android:layout_width="12.3dp"
                    android:layout_height="12dp"
                    android:layout_marginRight="9.7dp"
                    android:background="@mipmap/star_solid"
                    android:id="@+id/rating_star_1"
                    />
                <ImageView
                    android:layout_width="12.3dp"
                    android:layout_height="12dp"
                    android:layout_marginRight="9.7dp"
                    android:background="@mipmap/star_solid"
                    android:id="@+id/rating_star_2"
                    />
                <ImageView
                    android:layout_width="12.3dp"
                    android:layout_height="12dp"
                    android:layout_marginRight="9.7dp"
                    android:background="@mipmap/star_blank"
                    android:id="@+id/rating_star_3"
                    />
                <ImageView
                    android:layout_width="12.3dp"
                    android:layout_height="12dp"
                    android:background="@mipmap/star_blank"
                    android:id="@+id/rating_star_4"
                    />

            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/right"
            android:layout_width="71dp"
            android:layout_height="match_parent"
            android:background="@color/IGU_Point_Color"
            >
            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clickable="true"
                android:gravity="center_horizontal|center_vertical"
                android:textSize="@dimen/sp_16_7"
                android:textColor="@color/Color_White"
                android:background="@color/IGU_Point_Color"
                android:text="@string/route_delete_Btn"
                android:id="@+id/route_delete_Btn"/>
        </RelativeLayout>

    </com.guanaj.easyswipemenulibrary.EasySwipeMenuLayout>
</android.support.v7.widget.CardView>